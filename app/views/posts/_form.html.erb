<div class="container">
<%= render 'errors', object: @post %>
<%= form_for @post, :html => {:multipart => true} do |f| %>
    <div class="field">
      <%= f.label :photo %><br />
      <%= f.file_field :photo %>
      <%= f.hidden_field :photo_cache %> 
      <%= f.hidden_field :user_id, value: current_user.id, class:'d-none'%>   
    </div>

    </div>
    <div class="actions">
      <%= f.submit %>
    </div>
  <% end %>